# https://segment.com/docs/connections/sources/catalog/libraries/server/python/
def get_analytics_payload(data, event_type):
    if event_type == "track":
        return [
            data["user"].id,
            data["event"],
            data["properties"],
            *data["secondary_data"]
        ]
    elif event_type == "view":
        return [
            data["user"].id,
            data["category"],
            data["name"],
            data["properties"],
            *data["secondary_data"]
        ]
    elif event_type == "identify":
        return [data["user"].id, data["id_traits"], *data["secondary_data"]]
    elif event_type == "alias":
        return [data["uuid"], data["user"].id, *data["secondary_data"]]
    elif event_type == "group":
        return [
            data["user"].id,
            data["group_id"],
            data["id_traits"],
            *data["secondary_data"]
        ]

    return None
